<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net471;netcoreapp2.0</TargetFrameworks>
    <Version>5.0.0-beta.7</Version>
    <OutputType>Exe</OutputType>
    <ServerGarbageCollection>true</ServerGarbageCollection>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <PlatformTarget>AnyCPU</PlatformTarget>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Program.fs" />
    <Content Include="paket.references" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Logary, Version=5.0.0.0, Culture=neutral, PublicKeyToken=null" Condition="'$(TargetFramework)' == 'net471'">
      <HintPath>..\Logary\bin\Release\net461\Logary.dll</HintPath>
    </Reference>
    <Reference Include="Logary, Version=5.0.0.0, Culture=neutral, PublicKeyToken=null" Condition="'$(TargetFramework)' == 'netcoreapp2.0'">
      <HintPath>..\Logary\bin\Release\netstandard2.0\Logary.dll</HintPath>
    </Reference>
  </ItemGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>